{
	"name": "n8n-nodes-smsmasivos",
	"version": "0.1.4",
	"description": "n8n node for sending SMS via SMS Masivos",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Hernan",
		"email": ""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hernanjh/n8n-nodes-smsmasivos.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
